{
  "name": "winter-love",
  "version": "0.0.1",
  "description": "Winter Love packages",
  "productName": "Winter Love",
  "author": "Bichi Kim <bichi@live.co.kr>",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "spa": "yarn workspace app run spa",
    "spa:build": "yarn workspace app run build",
    "capacitor": "yarn workspace app run capacitor",
    "capacitor:build": "yarn workspace app run build:capacitor",
    "pwa": "yarn workspace app run pwa",
    "pwa:build": "yarn workspace app run build:pwa",
    "setup": "yarn --ignore-engines",
    "update": "yarn upgrade --ignore-engines",
    "deploy": "firebase deploy",
    "test:app": "yarn workspace app run test",
    "test:firebase": "mocha \"test/firebase/**/*.spec.ts\"",
    "firebase:emulator": "firebase serve --only firestore,database"
  },
  "scriptsDescription": {
    "spa": "serve app as spa mode",
    "spa:build": "build app as spa mode",
    "capacitor": "serve app as capacitor mode",
    "capacitor:build": "build app as capacitor mode",
    "pwa": "yarn workspace app run pwa",
    "pwa:build": "yarn workspace app run build:pwa",
    "setup": "install dependencies owing to packages/functions has wrong node engin version (won't support >12)",
    "update": "upgrade dependencies owing to packages/functions has wrong node engin version (won't support >12)",
    "deploy": "deploy to firebase host",
    "test:app": "run app unit test",
    "test:firebase": "run firebase test [WIP]",
    "firebase:emulator": "run firebase emulator"
  },
  "devDependencies": {
    "@firebase/testing": "^0.16.0",
    "@types/chai": "^4.1.7",
    "@types/cross-spawn": "^6.0.1",
    "@types/jsdom": "^12.2.3",
    "@types/lodash": "^4.14.144",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12",
    "@types/webpack": "^4.4.32",
    "@types/webpack-env": "^1.13.9",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "chai": "^4.2.0",
    "cross-spawn": "^7.0.1",
    "dotenv": "^8.0.0",
    "eslint": "^6.5.1",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-sort-class-members": "^1.5.0",
    "eslint-plugin-vue": "^6.0.1",
    "firebase-tools": "^7.8.1",
    "mocha": "^6.2.2",
    "mochapack": "^1.1.8",
    "tree-kill": "^1.2.1",
    "ts-loader": "^6.0.2",
    "ts-node": "^8.5.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.5.1",
    "vue-loader": "^15.7.0",
    "vuepress": "^1.2.0"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ],
  "main": "index.js",
  "repository": "git@github.com:bichikim/winter-love-quasar.git",
  "license": "MIT"
}
